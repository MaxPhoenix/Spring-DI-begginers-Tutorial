<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
       xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <!-- Definition for textEditor bean -->
    <bean id = "textEditor" class = "com.spring.tutorialspoint.XMLConfiguration.autowiring.TextEditor" autowire = "byType">
        <property name = "name" value = "Generic Text Editor" />
    </bean>

    <!-- the id has to match the FIELD TYPE  of TextEditor-->
    <!-- Definition for spellChecker bean -->
    <bean id = "spellChecker" class = "com.spring.tutorialspoint.XMLConfiguration.autowiring.SpellChecker"></bean>




    <bean id = "printer" class = "com.spring.tutorialspoint.XMLConfiguration.autowiring.Printer"></bean>


</beans>