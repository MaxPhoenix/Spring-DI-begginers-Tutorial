<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
       xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <!-- Definition for textEditor bean -->
    <bean id = "textEditor" class = "com.spring.tutorialspoint.DI.constructorBasedDI.TextEditor" autowire = "constructor">
    </bean>

    <!-- the id has to match the constructor ARGUMENT of TextEditor-->
    <!-- Definition for spellChecker bean -->
    <bean id = "spellChecker3" class = "com.spring.tutorialspoint.DI.constructorBasedDI.SpellChecker"></bean>

    <bean id = "spellChecker2" class = "com.spring.tutorialspoint.DI.constructorBasedDI.SpellChecker"></bean>

</beans>